<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item
        android:gravity="center">
        <shape
            android:shape="line">
            <solid
                android:color="#00000000"/>
            <size
                android:width="300dp"
                android:height="300dp"/>
            <stroke
                android:width="1px"
                android:color="#55fbfbfc"/>
        </shape>
    </item>
    <item
        android:gravity="center">
        <rotate
            android:pivotX="50%"
            android:pivotY="50%"
            android:fromDegrees="90"
            android:toDegrees="90">
            <shape
                android:shape="line">
                <solid
                    android:color="#00000000"/>
                <size
                    android:width="300dp"
                    android:height="300dp"/>
                <stroke
                    android:width="1px"
                    android:color="#55fbfbfc"/>
            </shape>
        </rotate>
    </item>
</layer-list>